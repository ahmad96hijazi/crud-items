<% if(locals.errors){ %> <% errors.forEach(function(error) { %>
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        <%= error.msg %>
        <button type="button" class="close btn-close btn" data-dismiss="alert" aria-label="Close">
        </button>
    </div>
<% }); %> <% } %> <% if(String(success_msg) !== ''){ %>
    <div class="alert alert-success alert-dismissible fade show" role="alert">
        <%= success_msg %>
        <button type="button" class="close btn-close btn" data-dismiss="alert" aria-label="Close">
        </button>
    </div>
<% } %> <% if(String(error_msg) !== ''){ %>
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        <%= error_msg %>
        <button type="button" class="close btn-close btn" data-dismiss="alert" aria-label="Close">
        </button>
    </div>
<% } %> <% if(String(error) !== ''){ %>
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        <%= error %>
        <button type="button" class="close btn-close btn btn-sm" data-dismiss="alert" aria-label="Close">
        </button>
    </div>
<% } %>
